@using Umbraco.Cms.Web.Common.PublishedModels;
@inherits Umbraco.Cms.Web.Common.Views.UmbracoViewPage
@{
	Layout = "mainLayout.cshtml";
}
<html lang="en">
    <head>
        <meta charset="UTF-8">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <link rel="stylesheet" href="/css/Projects.css">
        <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous">
        <script src="https://kit.fontawesome.com/1a29b24407.js" crossorigin="anonymous"></script>
        <title>Document</title>
    </head>
<body>

    <div class="wrapper">
        <header>
            <div class="GreenBox">
                <div class="GreenBoxInput">
                    <div class="AddressText">
                        <p>Klarabergsviadukten 90, 111 64 Stockholm</p>
                    </div>

                    <div class="SocialMediaButtons">
                        <a href="#"><div><i class="fa-brands fa-facebook"></i></div></a>
                        <a href="#"><div><i class="fa-brands fa-twitter"></i></div></a>
                        <a href="#"><div><i class="fa-brands fa-linkedin-in"></i></div></a>
                    </div>    
                </div>
                
            </div>

            <div class="WhiteBox">
                <div class="LogoAndButtons">
                    <div class="ImageLogo">
                        <a href="/Home">
                          @if (Model.Value<IPublishedContent>("headerLogo") != null)
                            {
                                var headerLogo = Model.Value<IPublishedContent>("headerLogo").Url();
                                <img src="@headerLogo" alt="headerLogo Image">   
                            }
                            else
                            {
                                <p>Ingen bild tillgänglig</p>
                            }  
                        </a>
                    </div>  
                    <div class="HeaderButtons">
                        <a href="/About">About</a>
                        <a href="/Services">Services</a>
                        <a href="/Projects">Projects</a>
                        <a href="/Contact">Contact</a>
                    </div>
                    
                </div>

                <div class="SearchBar">
                    <input type="text" placeholder="Search..">
                    <a href="#"><i class="fa-solid fa-magnifying-glass"></i></a>
                </div>
                

            </div>
        </header> 

        <main>

        <section class="OurProjects">

            <div class="OurProjectsBox">
                <div class="OurProjectTextBox">
                    <h2>
                        @Model.Value("ourProjectTitle")
                    </h2>
                    <p>
                        @Model.Value("ourProjectText")
                    </p>

                </div>
            </div>

        </section>

        <section class="OurSupport">
            <div class="OurSupportParentBox">
                <div class="OurSupportTextBoxes">
                    <div class="OurSupportTextBox">
                        @if (Model.Value<IPublishedContent>("ourSupportBoxOnePicture") != null)
                            {
                                var ourSupportBoxOnePicture = Model.Value<IPublishedContent>("ourSupportBoxOnePicture").Url();
                                <img src="@ourSupportBoxOnePicture" alt="ourSupportBoxOnePicture">   
                            }
                            else
                            {
                                <p>Ingen bild tillgänglig</p>
                            }  
                        
                        <div>
                            <h5>
                               @Model.Value("ourSupportBoxOneTitle")
                            </h5>

                            <p>
                                <i class="fa-solid fa-calendar-days"></i>
                                June 15, 2024
                            </p>

                            <p>
                                @Model.Value("ourSupportBoxOneText")
                            </p>

                            <a href="#">Read more
                                <i class="fa-solid fa-arrow-right"></i>
                            </a>
                        </div>

                    </div>

                    <hr>

                    <div class="OurSupportTextBox">
                         @if (Model.Value<IPublishedContent>("ourSupportBoxTwoPicture") != null)
                            {
                                var ourSupportBoxTwoPicture = Model.Value<IPublishedContent>("ourSupportBoxTwoPicture").Url();
                                <img src="@ourSupportBoxTwoPicture" alt="ourSupportBoxTwoPicture">   
                            }
                            else
                            {
                                <p>Ingen bild tillgänglig</p>
                            } 
                        
                        <div>
                            <h5>
                               @Model.Value("ourSupportBoxTwoTitle")
                            </h5>

                            <p>
                                <i class="fa-solid fa-calendar-days"></i>
                                May 17, 2024
                            </p>

                            <p>
                                @Model.Value("ourSupportBoxTwoText")
                            </p>

                            <a href="#">Read more
                                <i class="fa-solid fa-arrow-right"></i>
                            </a>
                        </div>

                    </div>

                    <hr>

                    <div class="OurSupportTextBox">
                         @if (Model.Value<IPublishedContent>("ourSupportBoxThreePicture") != null)
                            {
                                var ourSupportBoxThreePicture = Model.Value<IPublishedContent>("ourSupportBoxThreePicture").Url();
                                <img src="@ourSupportBoxThreePicture" alt="ourSupportBoxThreePicture">   
                            }
                            else
                            {
                                <p>Ingen bild tillgänglig</p>
                            } 
                        
                        <div>
                            <h5>
                                @Model.Value("ourSupportBoxThreeTitle")
                            </h5>

                            <p>
                                <i class="fa-solid fa-calendar-days"></i>
                                February 6, 2024
                            </p>

                            <p>
                                @Model.Value("ourSupportBoxThreeText")
                            </p>

                            <a href="#">Read more
                                <i class="fa-solid fa-arrow-right"></i>
                            </a>
                        </div>

                    </div>

                    <hr>

                    <div class="OurSupportTextBox">
                        @if (Model.Value<IPublishedContent>("ourSupportBoxFourPicture") != null)
                            {
                                var ourSupportBoxFourPicture = Model.Value<IPublishedContent>("ourSupportBoxFourPicture").Url();
                                <img src="@ourSupportBoxFourPicture" alt="ourSupportBoxFourPicture">   
                            }
                            else
                            {
                                <p>Ingen bild tillgänglig</p>
                            } 
                        
                        <div>
                            <h5>
                               @Model.Value("ourSupportBoxFourTitle")
                            </h5>

                            <p>
                                <i class="fa-solid fa-calendar-days"></i>
                                January 3, 2024
                            </p>

                            <p>
                                @Model.Value("ourSupportBoxFourText")
                            </p>

                            <a href="#">Read more
                                <i class="fa-solid fa-arrow-right"></i>
                            </a>
                        </div>

                    </div>
                </div>
                
                <div class="VerticalLine"></div>

                <div class="RecentProjects">
                    <h4>@Model.Value("recentProjectsTitle")</h4>

                    <div class="RecentBox">
                        @if (Model.Value<IPublishedContent>("recentBoxOnePicture") != null)
                            {
                                var recentBoxOnePicture = Model.Value<IPublishedContent>("recentBoxOnePicture").Url();
                                <img src="@recentBoxOnePicture" alt="recentBoxOnePicture">   
                            }
                            else
                            {
                                <p>Ingen bild tillgänglig</p>
                            } 

                        <div>
                            <h6>
                                @Model.Value("recentBoxOneTitle")
                            </h6>
                            <div class="RecentProjectsCalender">
                                <i class="fa-regular fa-calendar-days"></i>
                                June 15,2024
                            </div>
                        </div>
                    </div>

                    <div class="RecentBox">
                        @if (Model.Value<IPublishedContent>("recentBoxTwoPicture") != null)
                            {
                                var recentBoxTwoPicture = Model.Value<IPublishedContent>("recentBoxTwoPicture").Url();
                                <img src="@recentBoxTwoPicture" alt="recentBoxTwoPicture">   
                            }
                            else
                            {
                                <p>Ingen bild tillgänglig</p>
                            } 
                            
                        <div>
                            <h6>
                               @Model.Value("recentBoxTwoTitle")
                            </h6>
                            <div class="RecentProjectsCalender">
                                <i class="fa-regular fa-calendar-days"></i>
                                May 17,2024
                            </div>
                        </div>
                        
                    </div>

                    <div class="RecentBox">
                         @if (Model.Value<IPublishedContent>("recentBoxThreePicture") != null)
                            {
                                var recentBoxThreePicture = Model.Value<IPublishedContent>("recentBoxThreePicture").Url();
                                <img src="@recentBoxThreePicture" alt="recentBoxThreePicture">   
                            }
                            else
                            {
                                <p>Ingen bild tillgänglig</p>
                            } 
                           
                        <div>
                            <h6>
                                @Model.Value("recentBoxThreeTitle")
                            </h6>
                            <div class="RecentProjectsCalender">
                                <i class="fa-regular fa-calendar-days"></i>
                                February 6,2024
                            </div>
                        </div>
                        
                    </div>

                    <div class="RecentBox">
                          @if (Model.Value<IPublishedContent>("recentBoxFourPicture") != null)
                            {
                                var recentBoxFourPicture = Model.Value<IPublishedContent>("recentBoxFourPicture").Url();
                                <img src="@recentBoxFourPicture" alt="recentBoxFourPicture">   
                            }
                            else
                            {
                                <p>Ingen bild tillgänglig</p>
                            } 
                          
                        <div>
                            <h6>
                                @Model.Value("recentBoxFourTitle")
                            </h6>
                            <div class="RecentProjectsCalender">
                                <i class="fa-regular fa-calendar-days"></i>
                                January 3,2024
                            </div>
                        </div>
                    </div>

                    <div class="AboutUsCornerBottom">
                        <h4>@Model.Value("aboutUsCornerTitle")</h4>
                        <p>
                            @Model.Value("aboutUsCornerText")
                        </p>

                        <div class="AboutUsCornerEmail">
                            <input type="email" placeholder="E-mail address">
                            <i class="fa-regular fa-envelope"></i>  
                        </div>
                        


                    </div>
                </div>
            </div> 
        </section>

        <section class="ContactInformation">
            <div class="ContactPicture">
               @if (Model.Value<IPublishedContent>("contactInformationPicture") != null)
                            {
                                var contactInformationPicture = Model.Value<IPublishedContent>("contactInformationPicture").Url();
                                <img src="@contactInformationPicture" alt="contactInformationPicture">   
                            }
                            else
                            {
                                <p>Ingen bild tillgänglig</p>
                            } 
                            
            </div>
    
            <div class="ContactText">
                <div>
                    <h2>
                        @Model.Value("contactInformationTitle")
                    </h2>
    
                    <div class="Contact">
                        <div class="ContactChild">
                            <i class="fa-solid fa-location-dot"></i>
                            <h6>@Model.Value("contactInformationLocation")</h6>
                        </div>
    
    
                        <p>@Model.Value("contactInformationAddress")</p>
                    </div>
    
    
                    <div class="Contact">
                        <div class="ContactChild">
                            <i class="fa-solid fa-phone"></i>
                            <h6>@Model.Value("contactInformationPhone")</h6>
                        </div> 
                        <p>@Model.Value("contactInformationPhoneNumber")</p>
                    </div>
    
                    <div class="Contact">
                        <div class="ContactChild">
                            <i class="fa-regular fa-envelope"></i>
                            <h6>@Model.Value("contactInformationEmail")</h6>
                        </div>
                        <p>@Model.Value("contactInformationEmailAddress")</p>
                    </div>
    
                </div>
            </div>
    
        </section>

        </main>
        
        <footer>
            <div class="WhiteBox">
                @if (Model.Value<IPublishedContent>("footerPicture") != null)
                            {
                                var footerPicture = Model.Value<IPublishedContent>("footerPicture").Url();
                                <img src="@footerPicture" alt="footerPicture">   
                            }
                            else
                            {
                                <p>Ingen bild tillgänglig</p>
                            } 
                            
            </div>
    
            <div class="BlackBox">
                <p>© Onatrix. All Rights Reserved. Credits to <span>Themesflat</span></p>
            </div>
        </footer>
    </div>
    
</body>
</html>