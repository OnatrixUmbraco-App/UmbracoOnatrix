                </div>

                <div class="OurExpertBottomBoxes">
                    <div class="OurExpertBottomBox">
                            @if (Model.Value<IPublishedContent>("ourExpertBoxFourPicture") != null)
                                {
                                    var ourExpertBoxFourPicture = Model.Value<IPublishedContent>("ourExpertBoxFourPicture").Url();
                                    <img src="@ourExpertBoxFourPicture" alt="ourExpertBoxFourPicture">   
                                }
                            else
                                {
                                    <p>Ingen bild tillgÃ¤nglig</p>
                                }
                        <h6>@Model.Value("ourExpertBoxFourTitle")</h6>
