}
<html lang="en">
    <head>
        <meta charset="UTF-8">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <link rel="stylesheet" href="/css/About.css">
        <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous">
        <script src="https://kit.fontawesome.com/1a29b24407.js" crossorigin="anonymous"></script>
        <title>Document</title>
    </head>
<body>
    <div class="wrapper">
        <header>
            <div class="GreenBox">
                <div class="GreenBoxInput">
                    <div class="AddressText">
                        <p>Klarabergsviadukten 90, 111 64 Stockholm</p>
                    </div>
    
                    <div class="SocialMediaButtons">
                        <a href="#"><div><i class="fa-brands fa-facebook"></i></div></a>
                        <a href="#"><div><i class="fa-brands fa-twitter"></i></div></a>
                        <a href="#"><div><i class="fa-brands fa-linkedin-in"></i></div></a>
                    </div>    
                </div>
                
            </div>
    
            <div class="WhiteBox">
                <div class="LogoAndButtons">
                    <div class="ImageLogo">
                        <a href="/Home">
                            @if (Model.Value<IPublishedContent>("headerLogo") != null)
                                {
                                    var headerLogo = Model.Value<IPublishedContent>("headerLogo").Url();
                                    <img src="@headerLogo" alt="Header Image">   
                                }
                                else
                                {
                                    <p>Ingen bild tillgÃ¤nglig</p>
                                }
                        </a>
                    </div>  
                    <div class="HeaderButtons">
                        <a href="/About">About</a>
                        <a href="/Services">Services</a>
                        <a href="/Projects">Projects</a>
                        <a href="/Contact">Contact</a>
                    </div>
                    
                </div>
    
                <div class="SearchBar">
                    <input type="text" placeholder="Search..">
                    <a href="#"><i class="fa-solid fa-magnifying-glass"></i></a>
                </div>
                
    
            </div>
        </header> 
    
        <main>
            <section class="AboutUs">
                <div class="AboutUsChildBox">
                    <div class="AboutUsText">
                        <h2>@Model.Value("aboutUsTitle")</h2>
                        <p>@Model.Value("aboutUsText")</p>
                    </div>
                    
                </div>
                

            </section>

            <section class="TinaFox">
                <div class="TinaFoxBio">
                            @if (Model.Value<IPublishedContent>("tinaFoxPicture") != null)
                                {
                                    var tinaFoxPicture = Model.Value<IPublishedContent>("tinaFoxPicture").Url();
                                    <img src="@tinaFoxPicture" alt="tinaFoxPicture">   
                                }
                            else
                                {
                                    <p>Ingen bild tillgÃ¤nglig</p>
                                }
                    <p>@Model.Value("tinaFoxText")</p>
                </div>

                <div class="TinaFoxText">
                    <h5>
                        @Model.Value("tinaFoxTitle")
                    </h5>

                    <p>
                       @Model.Value("tinaFoxMainText")
                    </p>

                </div>

            </section>

            <section class="Reviews">
                <div class="ReviewsText">
                    <p id="ReviewsGoldText">@Model.Value("reviewsTitle")</p>

                    <h3>
                        @Model.Value("reviewsText")
                    </h3>

                    <p>
                        @Model.Value("reviewsMainText")
                    </p>

                    <span class="fa fa-star checked"></span>
                    <span class="fa fa-star checked"></span>
                    <span class="fa fa-star checked"></span>
                    <span class="fa fa-star checked"></span>
                    <span class="fa fa-star checked"></span>

                    <h6>Kevin Gardner</h6>
                    <h6>Swebank</h6>
                </div>

                <div class="ReviewsPic">
                            @if (Model.Value<IPublishedContent>("reviewsPicture") != null)
                                {
                                    var reviewsPicture = Model.Value<IPublishedContent>("reviewsPicture").Url();
                                    <img src="@reviewsPicture" alt="reviewsPicture">   
                                }
                            else
                                {
                                    <p>Ingen bild tillgÃ¤nglig</p>
                                }
                </div>

            </section>

            <section class="OurExpert">
              <div class="OurExperText">
                <div>
                    <p>@Model.Value("ourExpertTitle")</p>
                    <h2>
                        @Model.Value("ourExpertText")
                    </h2>
                </div>
              </div>

                <div class="OurExpertTopBoxes">
                    <div class="OurExpertTopBox">
                            @if (Model.Value<IPublishedContent>("ourExpertBoxOnePicture") != null)
