                        <h6>@Model.Value("ourExpertBoxSixTitle")</h6>
                        <p>@Model.Value("ourExpertBoxSixText")</p>
                    </div>

                    <div class="OurExpertBottomBox">
                            @if (Model.Value<IPublishedContent>("ourExpertBoxSevenPicture") != null)
                                {
                                    var ourExpertBoxSevenPicture = Model.Value<IPublishedContent>("ourExpertBoxSevenPicture").Url();
                                    <img src="@ourExpertBoxSevenPicture" alt="ourExpertBoxSevenPicture">   
