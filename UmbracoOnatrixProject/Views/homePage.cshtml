                        <div class="OurProjectGreenBox">
                            <h6>
                                @Model.Value("ourProjectBoxTwoTitle")
                            </h6>
                            <p>
                             @Model.Value("ourProjectBoxTwoText")
                            </p>
                         </div>
                    </div>

                    <div class="OurProjectPicBox">
                        @if (Model.Value<IPublishedContent>("ourProjectBoxThreePicture") != null)
                            {
                                var ourProjectBoxThreePicture = Model.Value<IPublishedContent>("ourProjectBoxThreePicture").Url();
                                <img src="@ourProjectBoxThreePicture" alt="ourProjectBoxThree Image">
                            }
                        else
                            {
                                <p>Ingen bild tillgänglig</p>
                            }
                        <div class="OurProjectGreenBox">
                            <h6>
                                @Model.Value("ourProjectBoxThreeTitle")
                            </h6>
                            <p>
                             @Model.Value("ourProjectBoxThreeText")
                            </p>
                         </div>
                    </div>

                    <div class="OurProjectPicBox">
                        @if (Model.Value<IPublishedContent>("ourProjectBoxFourPicture") != null)
                            {
                                var ourProjectBoxFourPicture = Model.Value<IPublishedContent>("ourProjectBoxFourPicture").Url();
                                <img src="@ourProjectBoxFourPicture" alt="ourProjectBoxFour Image">
                            }
                        else
                            {
                                <p>Ingen bild tillgänglig</p>
                            }
                        <div class="OurProjectGreenBox">
                            <h6>
                               @Model.Value("ourProjectBoxFourTitle")
                            </h6>
                            <p>
                             @Model.Value("ourProjectBoxFourText")
                            </p>
                         </div>
                    </div>


                </div>

            </section>


        </main>

        <footer>
            <div class="WhiteBox">
                @if (Model.Value<IPublishedContent>("footerImage") != null)
                {
                    var footerImage = Model.Value<IPublishedContent>("footerImage").Url();
                    <img src="@footerImage" alt="footerImage">
                }
                else
                {
                    <p>Ingen bild tillgänglig</p>
                }                
               
            </div>
    
            <div class="BlackBox">
                <p>© Onatrix. All Rights Reserved. Credits to <span>Themesflat</span></p>
            </div>
        </footer>
        </div>
